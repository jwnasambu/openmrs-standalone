            <column name="concept_id" type="int">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="handler" type="varchar(255)"/>
        </createTable>
		
		<sql>SET FOREIGN_KEY_CHECKS=1</sql>
	</changeSet>
	
